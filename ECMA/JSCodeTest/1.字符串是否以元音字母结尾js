/**
 * 浪费内存
 */
function endsWithVowel(str){
        let arr = ['a','e','i','o','u']
        let result = arr.some(item=>{
            console.log(str.length-1);
            return str[str.length-1].toLowerCase() === item.toLowerCase()
        })
        return result
}


function xxx(str){
    return /[a,e,i,o,u,A,E,I,O,U]$/.test(str)
}
